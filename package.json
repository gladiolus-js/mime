{
    "name": "@gladiolus/mime",
    "version": "0.1.0",
    "author": "lopo<lopo@zju.edu.cn>",
    "description": "determining the MIME type for a file",
    "license": "MIT",
    "packageManager": "yarn@3.6.4",
    "scripts": {
        "build": "tsup src/index.ts",
        "test": "ava"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "repository": {
        "type": "git",
        "url": "https://github.com/gladiolus-js/mime"
    },
    "exports": {
        "default": "./dist/index.js",
        "require": "./dist/index.js",
        "node": "./dist/index.js",
        "import": "./dist/index.mjs"
    },
    "ava": {
        "files": [
            "__test__/*.spec.mjs"
        ]
    },
    "devDependencies": {
        "ava": "^5.3.1",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2"
    },
    "files": [
        "dist",
        "src",
        "README.md",
        "LICENSE",
        "package.json"
    ],
    "publishConfig": {
        "access": "public"
    }
}
